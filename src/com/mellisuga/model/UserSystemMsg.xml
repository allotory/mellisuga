<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mellisuga.dao.UserSystemMsgDAO">
    <!-- 插入 -->
    <!-- useGeneratedKeys设置为"true"表明要MyBatis获取由数据库自动生成的主键；
    	 keyProperty="id"指定把获取到的主键值注入到UserSystemMsg的id属性--> 
    <insert id="insertUserSystemMsg" parameterType="UserSystemMsg" useGeneratedKeys="true" keyProperty="id">
    	insert into user_system_msg(member_id, system_message_id) 
    	values(#{member_id}, #{system_message_id})
    </insert>
</mapper>


